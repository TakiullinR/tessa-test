[TEXTPART SiRoutetypes MSSQL]


[TEXTPART SiRoutetypes PGSQL]
SELECT
	"r"."ID" as "SiID",
	"r"."Name" as "SiName",
	string_agg("SiDocumentTypesCaption", ', ') as "SiDocumentTypesCaption"
	
FROM "SiDoctypesroute" AS "t"
FULL JOIN "SiInfo" AS "r"
		ON "r"."ID" = "t"."ID"
CROSS JOIN "Localization"("t"."DocumentTypesCaption", #param(locale)) AS "SiDocumentTypesCaption"

WHERE true
	#param(SiID, "t"."ID")
	#param(SiName, "r"."Name")
	#param(SiDocumentTypesCaption, "t"."DocumentTypesCaption")
	#if(!ShowHidden || ShowHidden.CriteriaName != "IsTrue") {
		AND "r"."Hidden" = false
		}
	
	group by "r"."ID"
	ORDER BY #order_by
	
#if(PageOffset) {
OFFSET #param(PageOffset) - 1 LIMIT #param(PageLimit)
}

